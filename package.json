{
  "name": "attrobj",
  "version": "1.1.0",
  "description": "Creates an object based on element attributes",
  "main": "dist/attrobj.js",
  "scripts": {
    "dev": "nswatch",
    "build": "mkdir -p dist && browserify lib/attrobj.js -t [ babelify --presets [ es2015 ] ] -o dist/attrobj.js -s attrobj",
    "test": "browserify -t [ babelify --presets [ es2015 ] ] test/attrobj.test.js | tape-run --browser phantomjs --render tap-spec",
    "lint": "eslint . --ignore-pattern dist",
    "prepublish": "npm run build"
  },
  "watch": {
    "lib/*.js": [
      "lint",
      "build",
      "build:test"
    ],
    "test/*.js": [
      "lint",
      "build:test"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/firstandthird/attrobj.git"
  },
  "keywords": [
    "attributes",
    "browser",
    "dom"
  ],
  "author": "First+Third",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/firstandthird/attrobj/issues"
  },
  "homepage": "https://github.com/firstandthird/attrobj#readme",
  "devDependencies": {
    "babel-preset-es2015": "^6.6.0",
    "babelify": "^7.3.0",
    "browserify": "^13.0.1",
    "concurrently": "^3.1.0",
    "core-js": "^2.4.1",
    "eslint": "^3.5.0",
    "eslint-config-firstandthird": "3.2.0",
    "eslint-plugin-import": "2.2.0",
    "nswatch": "^0.2.0",
    "phantomjs": "^2.1.7",
    "static-server": "^2.0.4",
    "tap-spec": "^4.1.1",
    "tape": "^4.6.2",
    "tape-run": "^2.1.5"
  },
  "eslintConfig": {
    "env": {
      "browser": true
    },
    "extends": "firstandthird"
  },
  "browserify": {
    "transform": [
      [
        "babelify",
        {
          "presets": [
            "es2015"
          ]
        }
      ]
    ]
  }
}
